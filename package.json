{
  "name": "master-bundler",
  "version": "2.0.0",
  "description": "A modern TypeScript bundler with support for multiple module formats",
  "main": "dist/index.js",
  "scripts": {
    "test": "node --loader ts-node/esm node_modules/.bin/jest",
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "npx tsx src/cli.ts",
    "docs:dev": "vitepress dev docs --port 3000 --host 0.0.0.0",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs --port 3000 --host 0.0.0.0",
    "docs:generate": "tsx src/cli-doc.ts generate",
    "docs:update": "npm run docs:generate && npm run docs:build",
    "lint": "eslint src"
  },
  "keywords": [
    "typescript",
    "bundler",
    "build-tool"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/chokidar": "^1.7.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.9",
    "@types/serve-index": "^1.9.4",
    "@types/ws": "^8.5.10",
    "autoprefixer": "^10.4.20",
    "chalk": "^5.2.0",
    "chokidar": "^4.0.3",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cssnano": "^7.0.6",
    "dotenv": "^16.3.1",
    "esbuild": "^0.15.13",
    "express": "^4.18.2",
    "fs-extra": "^10.1.0",
    "glob": "^10.3.10",
    "http-proxy-middleware": "^2.0.6",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "open": "^9.1.0",
    "postcss": "^8.5.3",
    "rollup": "^3.23.0",
    "@rollup/plugin-terser": "^1.0.0",
    "sass": "^1.85.1",
    "serve-index": "^1.9.1",
    "sharp": "^0.33.5",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.0.4",
    "ws": "^8.15.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "marked": "^9.1.5",
    "vitepress": "^1.6.3",
    "vue": "latest"
  },
  "type": "module"
}